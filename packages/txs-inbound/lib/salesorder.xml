        <AmazonSiteNumber />
        <BillingCity />
        <BillingCountry />
        <BillingState />
        <BillingStreet />
        <BillingZip />
        <CurrencyType />
        <CustomerName />
        <CustomerOrderNumber />
        <CustomerPhone />
        <GiftMessage />
        <OrderDate type="datetime" />
        <PurchaseOrderNumber />
        <RequestedDeliveryDate type="datetime" />
        <RequestedShippingDate type="datetime" />
        <ShippingAddressType />
        <ShippingCity />
        <ShippingCountry />
        <ShippingMethod />
        <ShippingName />
        <ShippingState />
        <ShippingStreet />
        <ShippingZip />
